package geosings2
{
	import mx.containers.Panel;
	

	public class MapCtrl extends Panel
	{
		private var map:MapCanv;
		
		public function MapCtrl()
		{
			//TODO: implement function
			super();
			map = new MapCanv();
			map.width=this.width;
			map.height=this.height;
			//Alert.show("");
			setStyle("backgroundColor","#ffffff");
			addChild(map);
		}
		
	}
}